@prefix : <https://w3id.org/fr/def/core#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix wot: <http://xmlns.com/wot/0.1/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<https://w3id.org/fr/def/core#> a owl:Ontology ;
	owl:imports <http://purl.org/spar/datacite/> ;
	rdfs:comment "This vocabulary represents scientific reviews. This model is used to annotate http://fairreviews.linkeddata.es/ data."@en ;
	owl:versionInfo "0.1.0" ;
	dcterms:created "2017-09-01"^^xsd:date ;
	dcterms:modified "2017-11-03"^^xsd:date ;
	dc:creator <https://w3id.org/people/idafensp> ;
	dcterms:issued "2017-11-07"^^xsd:date ;
	vann:preferredNamespaceUri "https://w3id.org/fr/def/core#" ;
	dc:title "FAIR reviews ontology"@en ;
	dc:publisher <http://www.oeg-upm.net/> ;
	vann:preferredNamespacePrefix "fr"^^xsd:date ;
	dc:creator <https://w3id.org/people/mpoveda> .
# 
# 
# #################################################################
# #
# #    Annotation properties
# #
# #################################################################
# 
# 
# http://purl.org/dc/terms/creator

dcterms:creator a owl:AnnotationProperty ;
	rdfs:subPropertyOf dc:creator ;
	rdfs:range dcterms:Agent .
# 
# http://purl.org/dc/terms/issued

dcterms:issued a owl:AnnotationProperty ;
	rdfs:subPropertyOf dc:date ;
	rdfs:range rdfs:Literal .
# 
# http://purl.org/dc/terms/language

dcterms:language a owl:AnnotationProperty ;
	rdfs:subPropertyOf dc:language ;
	rdfs:range dcterms:LinguisticSystem .
# 
# http://purl.org/dc/terms/license

dcterms:license a owl:AnnotationProperty ;
	rdfs:subPropertyOf dc:rights ;
	rdfs:range dcterms:LicenseDocument .
# 
# http://purl.org/dc/terms/modified

dcterms:modified a owl:AnnotationProperty ;
	rdfs:subPropertyOf dc:date ;
	rdfs:range rdfs:Literal .
# 
# http://www.w3.org/2004/02/skos/core#prefLabel

<http://www.w3.org/2004/02/skos/core#prefLabel> a owl:AnnotationProperty .
# 
# 
# 
# #################################################################
# #
# #    Datatypes
# #
# #################################################################
# 
# 
# http://www.w3.org/2001/XMLSchema#date

xsd:date a rdfs:Datatype .
# 
# 
# 
# #################################################################
# #
# #    Object Properties
# #
# #################################################################
# 
# 
# http://purl.org/dc/terms/creator

dcterms:creator a owl:ObjectProperty .
# 
# http://purl.org/dc/terms/language

dcterms:language a owl:ObjectProperty .
# 
# http://purl.org/dc/terms/license

dcterms:license a owl:ObjectProperty .
# 
# http://www.w3.org/2004/02/skos/core#closeMatch

<http://www.w3.org/2004/02/skos/core#closeMatch> a owl:ObjectProperty .
# 
# http://www.w3.org/2004/02/skos/core#inScheme

<http://www.w3.org/2004/02/skos/core#inScheme> a owl:ObjectProperty .
# 
# https://w3id.org/fr/def/core#hasConfidence

:hasConfidence a owl:ObjectProperty ;
	rdfs:comment "relation between a review author and the confidence about the review topic" ;
	rdfs:label "has confidence"@en .
# 
# https://w3id.org/fr/def/core#hasRating

:hasRating a owl:ObjectProperty ;
	rdfs:domain :Review ;
	rdfs:range :RatingConcept ;
	rdfs:comment "The relation between a review and a given rating assigned by a reviewer."@en ;
	rdfs:label "has rating"@en .
# 
# https://w3id.org/fr/def/core#hasReview

:hasReview a owl:ObjectProperty ;
	owl:inverseOf :isReviewOf ;
	rdfs:domain <http://purl.org/spar/fabio/Expression> ;
	rdfs:range :Review ;
	rdfs:comment "Relation between a scientific contribution and the reviews issued for it."@en ;
	rdfs:label "has review"@en .
# 
# https://w3id.org/fr/def/core#isReplyTo

:isReplyTo a owl:ObjectProperty ;
	rdfs:domain <http://purl.org/spar/fabio/Endeavour> ;
	rdfs:range <http://purl.org/spar/fabio/Endeavour> ;
	rdfs:comment "The relation between a given endeavour and the endeavour that it is replying to. For example, the relation between a reply and a given review."@en ;
	rdfs:label "is reply to"@en .
# 
# https://w3id.org/fr/def/core#isReviewOf

:isReviewOf a owl:ObjectProperty ;
	rdfs:domain :Review ;
	rdfs:range <http://purl.org/spar/fabio/Expression> ;
	rdfs:comment "The relation between a review and the scientific contribution that is subject of the review."@en ;
	rdfs:label "is review of"@en .
# 
# https://w3id.org/fr/def/core#issuedAt

:issuedAt a owl:ObjectProperty ;
	rdfs:domain :Review ;
	rdfs:range :TrackingSystem ;
	rdfs:comment "The relation between a review and the tracking or scholarly venue management system where the reviewing process took place."@en ;
	rdfs:label "issued at"@en .
# 
# https://w3id.org/fr/def/core#issuedFor

:issuedFor a owl:ObjectProperty ;
	rdfs:domain :Review ;
	rdfs:range :ConveningOrganization ;
	rdfs:comment "The realation between a review and the entity (e.g. conference, journal) that it was issued for (usually the one that requested the review)."@en ;
	rdfs:label "issued for"@en .
# 
# https://w3id.org/fr/def/core#releasedBy

:releasedBy a owl:ObjectProperty ;
	rdfs:domain :Review ;
	rdfs:range foaf:Agent ;
	rdfs:comment "The relation between the review and the agent publishing it. It might be the the author of the review or conference or journal to which the scientific resource was submitted to."@en ;
	rdfs:label "released by"@en .
# 
# 
# 
# #################################################################
# #
# #    Data properties
# #
# #################################################################
# 
# 
# http://purl.org/dc/terms/issued

dcterms:issued a owl:DatatypeProperty .
# 
# http://purl.org/dc/terms/modified

dcterms:modified a owl:DatatypeProperty .
# 
# http://purl.org/spar/fabio/hasURL

<http://purl.org/spar/fabio/hasURL> a owl:DatatypeProperty ;
	rdfs:domain <http://purl.org/spar/fabio/Endeavour> ;
	rdfs:range xsd:anyURI .
# 
# https://w3id.org/fr/def/core#confidenceValue

:confidenceValue a owl:DatatypeProperty ;
	rdfs:domain :ConfidenceConcept ;
	rdfs:range rdfs:Literal ;
	rdfs:comment "Confidence value of expertise in the review topic given by the reviwer"@en ;
	rdfs:label "confidence value"@en .
# 
# https://w3id.org/fr/def/core#content

:content a owl:DatatypeProperty ;
	rdfs:domain <http://purl.org/spar/fabio/Work> ;
	rdfs:range rdfs:Literal ;
	rdfs:comment "Content of the review."@en ;
	rdfs:label "content"@en .
# 
# https://w3id.org/fr/def/core#ratingValue

:ratingValue a owl:DatatypeProperty ;
	rdfs:domain :RatingConcept ;
	rdfs:range rdfs:Literal ;
	rdfs:comment "Numerical value of the rating given to a review."@en ;
	rdfs:label "rating value"@en .
# 
# 
# 
# #################################################################
# #
# #    Classes
# #
# #################################################################
# 
# 
# http://lexvo.org/ontology#Language

<http://lexvo.org/ontology#Language> a owl:Class ;
	rdfs:comment "Language of the scientific contribution (paper, review, etc.)."@en ;
	rdfs:label "Language"@en .
# 
# http://purl.org/dc/terms/Agent

dcterms:Agent a owl:Class .
# 
# http://purl.org/spar/bido/ScholarlyVenueManagementSystem

<http://purl.org/spar/bido/ScholarlyVenueManagementSystem> a owl:Class .
# 
# http://purl.org/spar/datacite/Identifier

<http://purl.org/spar/datacite/Identifier> a owl:Class ;
	rdfs:comment "Un identificador que identifica de forma única a una entidad - tal como una agencia de financiación, una persona y un recurso - que pertenece a un esquema particular, como los especificados por individuos de la clase datacite:IdentifierScheme."@es ;
	rdfs:label "identificador"@es .
# 
# http://purl.org/spar/fabio/Endeavour

<http://purl.org/spar/fabio/Endeavour> a owl:Class .
# 
# http://purl.org/spar/fabio/Expression

<http://purl.org/spar/fabio/Expression> a owl:Class ;
	rdfs:subClassOf <http://purl.org/spar/fabio/Endeavour> .
# 
# http://purl.org/spar/fabio/Reply

<http://purl.org/spar/fabio/Reply> a owl:Class ;
	rdfs:subClassOf <http://purl.org/spar/fabio/Work> .
# 
# http://purl.org/spar/fabio/Review

<http://purl.org/spar/fabio/Review> a owl:Class ;
	rdfs:subClassOf <http://purl.org/spar/fabio/Work> .
# 
# http://purl.org/spar/fabio/Work

<http://purl.org/spar/fabio/Work> a owl:Class ;
	rdfs:subClassOf <http://purl.org/spar/fabio/Endeavour> .
# 
# http://www.w3.org/2004/02/skos/core#ConceptScheme

<http://www.w3.org/2004/02/skos/core#ConceptScheme> a owl:Class .
# 
# http://www.w3.org/ns/odrl/2/Policy

<http://www.w3.org/ns/odrl/2/Policy> a owl:Class ;
	rdfs:comment "A non-empty group of Permissions and/or Prohibitions."@en ;
	rdfs:label "Policy"@en .
# 
# http://xmlns.com/foaf/0.1/Agent

foaf:Agent a owl:Class ;
	rdfs:isDefinedBy <http://xmlns.com/foaf/0.1/> .
# 
# http://xmlns.com/foaf/0.1/Person

foaf:Person a owl:Class ;
	rdfs:subClassOf foaf:Agent ;
	rdfs:isDefinedBy <http://xmlns.com/foaf/0.1/> .
# 
# https://w3id.org/fr/def/core#ConfidenceConcept

:ConfidenceConcept a owl:Class ;
	rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#Concept> ;
	rdfs:comment "The set of values of a given confidence scale."@en ;
	rdfs:label "Confidence concept"@en .
# 
# https://w3id.org/fr/def/core#ConfidenceConceptScheme

:ConfidenceConceptScheme a owl:Class ;
	rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#ConceptScheme> ;
	rdfs:comment "A scale defining the confidence of a reviewer for a given topic."@en ;
	rdfs:label "Confidence concept scheme"@en .
# 
# https://w3id.org/fr/def/core#ConveningOrganization

:ConveningOrganization a owl:Class ;
	rdfs:subClassOf foaf:Agent ;
	rdfs:comment "A venue (e.g. conference, workshop), a expression collection (e.g. journal, special issue), or an individual agent (e.g. organization, researcher) collecting scientific contributions."@en ;
	rdfs:label "Convening organization"@en .
# 
# https://w3id.org/fr/def/core#DiscreteConfidenceConceptScheme

:DiscreteConfidenceConceptScheme a owl:Class ;
	rdfs:subClassOf :ConfidenceConceptScheme ;
	rdfs:comment "A discrete scale defining the confidence of a reviewer for a given topic."@en ;
	rdfs:label "Discrete confidence concept scheme"@en .
# 
# https://w3id.org/fr/def/core#DiscreteRatingConceptScheme

:DiscreteRatingConceptScheme a owl:Class ;
	rdfs:subClassOf :RatingConceptScheme ;
	rdfs:comment "A discrete scale defining the rating given by a reviewer for a specific scientific contribution."@en ;
	rdfs:label "Discrete rating concept scheme"@en .
# 
# https://w3id.org/fr/def/core#RatingConcept

:RatingConcept a owl:Class ;
	rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#Concept> ;
	rdfs:comment "The set of values of a given rating scale."@en ;
	rdfs:label "Rating concept"@en .
# 
# https://w3id.org/fr/def/core#RatingConceptScheme

:RatingConceptScheme a owl:Class ;
	rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#ConceptScheme> ;
	rdfs:comment "A scale defining the rating given by a reviewer for a specific scientific contribution."@en ;
	rdfs:label "Rating concept scheme"@en .
# 
# https://w3id.org/fr/def/core#Reply

:Reply a owl:Class ;
	rdfs:subClassOf <http://purl.org/spar/fabio/Reply> , _:genid1 .

_:genid1 a owl:Restriction ;
	owl:onProperty dcterms:creator ;
	owl:someValuesFrom foaf:Agent .

:Reply rdfs:subClassOf _:genid2 .

_:genid2 a owl:Restriction ;
	owl:onProperty dcterms:language ;
	owl:someValuesFrom <http://lexvo.org/ontology#Language> .

:Reply rdfs:subClassOf _:genid3 .

_:genid3 a owl:Restriction ;
	owl:onProperty dcterms:issued ;
	owl:someValuesFrom xsd:date .

:Reply rdfs:subClassOf _:genid4 .

_:genid4 a owl:Restriction ;
	owl:onProperty dcterms:modified ;
	owl:someValuesFrom xsd:date .

:Reply rdfs:comment "A reply to a review or another endeavour."@en ;
	rdfs:label "Reply"@en .
# 
# https://w3id.org/fr/def/core#Review

:Review a owl:Class ;
	rdfs:subClassOf <http://purl.org/spar/fabio/Review> , _:genid5 .

_:genid5 a owl:Restriction ;
	owl:onProperty dcterms:creator ;
	owl:someValuesFrom foaf:Agent .

:Review rdfs:subClassOf _:genid6 .

_:genid6 a owl:Restriction ;
	owl:onProperty dcterms:language ;
	owl:someValuesFrom <http://lexvo.org/ontology#Language> .

:Review rdfs:subClassOf _:genid7 .

_:genid7 a owl:Restriction ;
	owl:onProperty dcterms:license ;
	owl:someValuesFrom <http://www.w3.org/ns/odrl/2/Policy> .

:Review rdfs:subClassOf _:genid8 .

_:genid8 a owl:Restriction ;
	owl:onProperty dcterms:issued ;
	owl:someValuesFrom xsd:date .

:Review rdfs:subClassOf _:genid9 .

_:genid9 a owl:Restriction ;
	owl:onProperty dcterms:modified ;
	owl:someValuesFrom xsd:date .

:Review rdfs:comment "A review of a scientific contribution."@en ;
	rdfs:label "Review"@en .
# 
# https://w3id.org/fr/def/core#TrackingSystem

:TrackingSystem a owl:Class ;
	rdfs:subClassOf <http://purl.org/spar/bido/ScholarlyVenueManagementSystem> ;
	rdfs:comment "The platform where the review process takes place."@en ;
	rdfs:label "Tracking system"@en .
